[application]

config/name="TSDC-Godot"
run/main_scene="res://Game.tscn"
config/icon="res://sprites/icon.png"

[autoload]

Global="*res://Global.gd"
Generators="*res://Generators.gd"
ItemLibrary="*res://ItemLibrary.gd"
ComponentLibrary="*res://components/ComponentLibrary.gd"
Status="*res://status/Status.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=720
window/size/height=480
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "equipment_watcher" )

